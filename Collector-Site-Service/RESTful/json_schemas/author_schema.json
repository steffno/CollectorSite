{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://www.univaq.it/f3i/swa/examples/author_schema.json",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "name",
        "recordno",
        "isband",
        "bio"
    ],
    "properties": {
        "name": {
            "type": "string"
        },

        "recordno": {
            "type": "integer"
        },

        "isband": {
            "type": "boolean"
        },

        "bio": {
            "type": "string"
        },

        "genreType": {
            "type": "string"
        }
    }
}